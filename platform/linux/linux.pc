Name: Linux musl libc
Version: 1.2.5
Description: Linux libc
Cflags: --sysroot=${build_prefix}/sysroot \
  -isystem ${build_prefix}/sysroot/usr/include \
  -isystem ${build_prefix}/linux_alpine/usr/include \
  -U_FORTIFY_SOURCE -D_FORTIFY_SOURCE=3
Libs: --sysroot=${build_prefix}/sysroot \
  -resource-dir=${build_prefix}/linux_alpine/compiler-rt
